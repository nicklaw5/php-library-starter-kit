    "keywords": ["library", "starter-kit"],
    "homepage": "http://github.com/nicklaw5/php-library-starter-kit",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Law",
            "homepage": "https://github.com/nicklaw5"
        }
    ],
    "require": {},
